{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run types:gen",
    "build": "vite build",
    "vite:build": "vite build",
    "check": "npm run -s lint && npm run -s vite:build",
    "prepare": "npx husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:https": "vite preview --host --https",
    "types:gen": "node -e \"console.log('Skipping types generation by default. Use `npm run types:gen:local` or `npm run types:gen:remote`.')\"",
    "types:gen:local": "supabase gen types typescript --local --schema public > src/lib/database.types.ts",
    "types:gen:remote": "supabase gen types typescript --project-id $env:SUPABASE_PROJECT_ID --schema public > src/lib/database.types.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.54.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^12.0.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.35",
    "supabase": "^2.34.3",
    "tailwindcss": "^3.4.1",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.2"
  }
}
